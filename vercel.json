{
  "version": 2,
  "builds": [
    {
      "src": "chatapp/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "chatapp/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "chatapp.settings"
  }
}
